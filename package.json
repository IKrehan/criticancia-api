{
  "name": "criticancia-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": { "node": "14.15.1" },
  "scripts": {
    "tsc": "tsc",
    "prestart": "rm -rf ./dist && tsc",
    "start": "node ./dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "build": "sucrase ./$npm_package_config_path_source -d ./$npm_package_config_path_dist --transforms typescript,imports",
    "build:dev": "sucrase ./src -d ./dist --transforms typescript,imports",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/sequelize": "^4.28.9",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "sucrase": "^3.17.0",
    "typescript": "^4.1.3"
  }
}
